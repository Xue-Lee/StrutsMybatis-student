<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="default" namespace="/" extends="struts-default">
        <action name="login" class="com.icss.action.StuAction" method="login">
            <result name="seccess" type="redirectAction">showCourse</result>
            <result name="login">login.jsp</result>
        </action>
        <action name="showCourse" class="com.icss.action.StuAction" method="showCourse">
            <result name="seccess">showCourse.jsp</result>
        </action>
    </package>
   
</struts>
